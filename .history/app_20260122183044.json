{
  "expo": {
    "name": "Uptrender",
    "slug": "uptrender",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/images/uptrender-logo.png",
    "scheme": "uptrender",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "raman42",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raman42.uptrender",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "uptrender"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/uptrender-logo.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/uptrender-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.raman42.uptrender",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "uptrender"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/uptrender-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/uptrender-logo.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ff706fec-d3a9-42e5-848b-91a2f46d6d3a"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c5e28dc7-6c66-4c88-811b-81a59d9bba3e"
      }
    }
  }
}
